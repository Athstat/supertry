{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "rm -rf node_modules/.vite &&  vite --host=0.0.0.0",
    "dev-win": "vite --host=0.0.0.0",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "jest",
    "format": "prettier --write .",
    "test:e2e": "npm run test:e2e:signin && npm run test:e2e:teams",
    "test:e2e:teams": "maestro test --config .maestro/config.yaml .maestro/flows/teams/06-edit-team.yaml",
    "test:e2e:signin": "maestro test --config .maestro/config.yaml .maestro/flows/auth/02-signin.yaml",
    "ci": "lint && test && build"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.17.3",
    "@react-oauth/google": "^0.12.2",
    "axios": "^1.8.2",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.6.0",
    "idb-keyval": "^6.2.2",
    "jotai": "^2.12.4",
    "jotai-scope": "^0.7.3",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.502.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-apple-signin-auth": "^1.1.2",
    "react-chartjs-2": "^5.3.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-image-crop": "^11.0.10",
    "react-intersection-observer": "^9.16.0",
    "react-mobile-app-button": "^1.2.19",
    "react-router-dom": "^6.29.0",
    "react-scan": "^0.4.3",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}